interface={{ wifi.interface }}
{% if wifi.bridge_mode %}
bridge=br0
{% else %}
driver=nl80211
{% endif %}
ssid={{ wifi.ssid }}
hw_mode={{ wifi.hw_mode }}
channel={{ wifi.channel }}
wmm_enabled=0
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_passphrase={{ wifi.passphrase }}
wpa_key_mgmt=WPA-PSK
wpa_pairwise=TKIP
rsn_pairwise=CCMP
